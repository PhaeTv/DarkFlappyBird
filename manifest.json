{
	"name": "Dark Flappy Bird",
	"description": "Tap or Touch the screen to play",
	"version": "1.0.0.0",
	"manifest_version": 2,
	"homepage_url": "http://phaetv.com",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"storage",
		"unlimitedStorage"
	],
	"offline_enabled": true
}